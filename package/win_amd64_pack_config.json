{
    "packMode": "jpackage",
    "platform": "win_amd64",
    "dest": "D:\\Package\\EasyZK\\${appVersion}\\EasyZK_${appVersion}",
    "appIcon": "D:\\Workspaces\\OYZH\\easyzk\\resource\\zoo.ico",
    "mainJar": "D:\\Workspaces\\OYZH\\easyzk\\target\\easyzk-${appVersion}.jar",
    "jdkPath": "D:\\Libraries\\JDKs\\openjdk-22_windows-x64_bin\\jdk-22",
    "packr": {
        "verbose": true,
        "mainClass": "org.springframework.boot.loader.JarLauncher",
        "classpath": []
    },
    "jpackage": {
        "verbose": true,
        "vendor": "oyzh",
        "type": "app-image",
        "description": "这是一个Zookeeper管理工具，提供导入、导出、传输、终端、可视化操作等功能."
    },
    "compress": {
        "type": "zip"
    },
    "jdeps": {
        "skips": [
            "javafx-.*.jar"
        ]
    },
    "jlink": {
        "excludeFiles": [
            "**.md",
            "**LICENSE",
            "**jvm.lib",
            "**jawt.lib",
            "**lcms.dll",
            "**jawt.dll",
            "**COPYRIGHT",
            "**prefs.dll",
            "**j2gss.dll",
            "**classlist",
            "**jsound.dll",
            "**README.txt",
            "**javajpeg.dll",
            "**mlib_image.dll",
            "**jrunscript.exe",
            "**sspi_bridge.dll",
            "**sound.properties",
            "**splashscreen.dll",
            "**w2k_lsa_auth.dll",
            "**ASSEMBLY_EXCEPTION",
            "**ADDITIONAL_LICENSE_INFO"
        ],
        "addModules": [
            "java.xml",
            "java.sql",
            "java.base",
            "java.naming",
            "java.logging",
            "java.desktop",
            "java.scripting",
            "java.management",
            "java.datatransfer",
            "java.security.jgss",
            "jdk.unsupported"
        ]
    },
    "jre": {
        "excludes": [
            "COPYRIGHT",
            "README.txt",
            "sound.properties",
            "ASSEMBLY_EXCEPTION",
            "ADDITIONAL_LICENSE_INFO",
            ".*.md",
            "lcms.*",
            "jawt.*",
            "j2gss.*",
            "prefs.*",
            "jsound.*",
            "LICENSE.*",
            "javajpeg.*",
            "classlist.*",
            "jrunscript.*",
            "splashscreen.*"
        ]
    },
    "jar": {
        "removeEmpty": true,
        "excludes": [
            "pom.xml",
            "pom.properties",
            "AUTHORS",
            "README.md",
            "README.txt",
            "DEPENDENCIES",
            "aot.factories",
            "changelog.txt",
            "module-info.class",
            "latestchanges.html",
            "package-info.class",
            "ASSEMBLY_EXCEPTION",
            "reflectionconfig.json",
            "release-timestamp.txt",
            "ADDITIONAL_LICENSE_INFO",
            "reactor.blockhound.integration.BlockHoundIntegration",
            "classpath.idx",
            "LICENSE.*",
            ".*.md",
            ".*.pro",
            ".*.xsd",
            "NOTICE.*",
            ".*.tooling",
            ".*.schemas",
            ".*.handlers",
            "lombok-.*.jar",
            ".*.processors",
            ".*.kotlin_module",
            ".*.dylib",
            ".*.so",
            "javafx-graphics.*-mac.jar",
            "javafx-graphics.*-linux.jar"
        ]
    }
}
